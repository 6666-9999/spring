<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cssl.dao.StreetMapper">
	<resultMap type="street" id="str">
		<id column="sid" property="sid"/>
		<association property="district" javaType="District">
			<id column="did" property="did"/>
		</association>
	</resultMap>
	<sql id="se">
		did,dname,sid,sname
	</sql>
	<select id="getAll" resultMap="str">
		select <include refid="se"/> from district d
		inner join street s on d.did=s.sdid
	</select>
</mapper>